    "Tags": "repo",
    "IP": "rep.litdev.me",
    "Port": "80",
    "IsHttps": false,
    "HealthCheckPath": "/api/health/check",
    "HealthInterval": 30
  },
  "StaticFilesDirectory": "D:\\WorkSpace\\GitHub\\MyNetCore\\MyNetCore.Web\\wwwroot",
  "VueProjectDirectory": "D:\\WorkSpace\\GitHub\\MyNetCore-Web", //前端项目所在目录，用于代码生成器
