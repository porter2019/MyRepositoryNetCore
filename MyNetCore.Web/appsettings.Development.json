    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  //配置文件必须有这项
  "ConsulKV": {
    "IsEnabled": false,
    "ServerUrl": "http://127.0.0.1:8500/",
    "Folder": "rep" //文件夹
  },
  //下面的都可以放到Consul的KV中配置
  "Consul": {
    "IP": "127.0.0.1",
    "Port": "8500"
  },
  "Service": {
    "AddConsulWatch": false,
    "SubId": "10000",
